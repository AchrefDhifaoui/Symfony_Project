{% extends 'template.html.twig' %}
{% block body %}
<table class="table">
    {% for note in notes %}
    <tr><td>
            {{ note }}

        </td></tr>
    {% endfor %}
</table>
<div class="alert alert-info">
    <p>nombre de note :  {{ notes | length}} </p>
    <p>note mini:  {{ min(notes)}} </p>
    <p>note max :  {{ max(notes)}} </p>
</div>
{% endblock %}