{% extends 'template.html.twig' %}

{% block title %}Formateur{% endblock %}

{% block body %}
    <h1>Formateur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ formateur.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ formateur.name }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ formateur.firstname }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ formateur.adresse }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ formateur.cin }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ formateur.tel }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ formateur.email }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ formateur.specialite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_formateur_index') }}">back to list</a>

    <a href="{{ path('app_formateur_edit', {'id': formateur.id}) }}">edit</a>

    {{ include('formateur/_delete_form.html.twig') }}
